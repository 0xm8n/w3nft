{
  "name": "pride-demons",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "lint": "prettier test/**/* examples/**/* scripts/**/* --check",
    "lint:fix": "prettier test/**/* examples/**/* scripts/**/* --write",
    "format": "prettier --check contracts/**/*.sol examples/**/*.sol",
    "format:fix": "prettier --write contracts/**/*.sol examples/**/*.sol",
    "clean": "hardhat clean",
    "build": "hardhat compile",
    "rebuild": "hardhat clean && hardhat compile",
    "deploy": "hardhat --network localhost run scripts/deploy-test.ts",
    "rinkeby:deploy": "hardhat --network rinkeby run scripts/deploy-test.ts",
    "rinkeby:verify": "export $(cat .env | xargs)&& hardhat verify --network rinkeby --constructor-args scripts/arguments-test.ts $CONTRACT",
    "goerli:deploy": "hardhat --network goerli run scripts/deploy-test.ts",
    "goerli:verify": "export $(cat .env | xargs)&& hardhat verify --network goerli --constructor-args scripts/arguments-test.ts $CONTRACT",
    "main:deploy": "hardhat --network mainnet run scripts/deploy.ts",
    "main:verify": "export $(cat .env | xargs)&& hardhat verify --network mainnet --constructor-args scripts/arguments.ts $CONTRACT",
    "test:gas": "cross-env REPORT_GAS=true hardhat test",
    "test": "hardhat test",
    "node": "hardhat node",
    "gensig": "hardhat run --no-compile scripts/signature.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@alch/alchemy-web3": "^1.1.11",
    "@chainlink/contracts": "^0.4.0",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@openzeppelin/contracts": "^4.4.2",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.42",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "erc721a": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.3",
    "hardhat": "^2.8.3",
    "hardhat-gas-reporter": "^1.0.7",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.18",
    "ts-node": "^10.4.0",
    "typechain": "^5.2.0",
    "typescript": "^4.5.5"
  }
}
